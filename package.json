{
  "name": "rtk-mock-store",
  "version": "1.0.0",
  "description": "Modern redux store jest mock helper function for testing reducers using @reduxjs/toolkit slices.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "jest": "jest --config=jest.config.js",
    "jest:coverage": "jest --config=jest.config.js --coverage",
    "tsup": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "test": "npm run jest",
    "test:coverage": "npm run jest:coverage",
    "build": "npm run tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidsalman/rtk-mock-store.git"
  },
  "keywords": [
    "redux",
    "toolkit",
    "mock",
    "store",
    "jest"
  ],
  "author": "David Salman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidsalman/rtk-mock-store/issues"
  },
  "homepage": "https://github.com/davidsalman/rtk-mock-store#readme",
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}
